{
  "name": "@flexis/ui",
  "version": "0.2.2-alpha",
  "description": "Styleless React Components",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/flexis-ui"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-ui/issues"
  },
  "scripts": {
    "createComponent": "./scripts/createComponent.sh",
    "lint:styles": "stylelint 'src/**/*.css'",
    "lint:scripts": "tslint -p . -t stylish 'src/**/*.{ts,tsx}'",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "test": "npm run lint && npm run build",
    "storybook": "start-storybook -p 3001 -c .storybook",
    "build:docs": "./scripts/generateDocs.sh",
    "build:storybook": "NODE_ENV=development build-storybook -c .storybook -o storybook-build",
    "build": "./scripts/buildPackage.sh",
    "prepublishOnly": "if [ -f .gitignore ]; then exit 1; fi",
    "cleanPublish": "npm run test && cd package && clean-publish"
  },
  "keywords": [
    "react-components",
    "ui",
    "styleless"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@stylable/runtime": "^0.1.8",
    "@types/prop-types": "^15.5.5",
    "@types/react": "16.7.7",
    "@types/react-transition-group": "^2.0.13",
    "react-input-mask": "^2.0.3",
    "react-transition-group": "^2.4.0",
    "resize-observer": "^1.0.0-alpha.1",
    "throttle-debounce": "^2.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@storybook/addon-a11y": "^4.0.9",
    "@storybook/addon-actions": "^4.0.0-alpha.18",
    "@storybook/addon-info": "^4.0.0-alpha.18",
    "@storybook/addon-knobs": "^4.0.0-alpha.18",
    "@storybook/addon-options": "^4.0.0-alpha.18",
    "@storybook/addons": "^4.0.0-alpha.18",
    "@storybook/react": "^4.0.0-alpha.18",
    "@stylable/core": "^0.1.10",
    "@stylable/webpack-plugin": "^0.1.11",
    "@types/node": "^10.9.3",
    "@types/storybook__addon-a11y": "^3.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.1",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "autoprefixer": "^9.1.3",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0",
    "babel-preset-trigen": "^1.7.1",
    "browserslist-config-trigen": "^1.1.0",
    "clean-publish": "^1.1.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.4.0",
    "eslint-config-trigen": "^3.2.0",
    "husky": "^1.0.0-rc.10",
    "lint-staged": "^8.1.0",
    "moment": "^2.22.2",
    "postcss": "^7.0.2",
    "postcss-momentum-scrolling": "^1.4.8",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "stylelint": "^9.5.0",
    "stylelint-config-trigen": "^2.0.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-react-loader": "^0.4.5",
    "tslint": "^5.11.0",
    "tslint-config-trigen": "^3.0.6",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.13.0",
    "typescript": "3.1.6"
  }
