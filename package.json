    "@storybook/addons": "^3.4.7",
    "@storybook/react": "^3.3.12",
    "autoprefixer": "^8.0.0",
    "awesome-typescript-loader": "^5.0.0",
    "eslint": "^4.19.1",
    "eslint-config-trigen": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "postcss-loader": "^2.1.0",
    "postcss-reporter": "^5.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
