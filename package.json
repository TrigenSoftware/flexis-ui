{
  "name": "@flexis/ui",
  "version": "0.1.3",
  "description": "Styleless React Components",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/flexis-ui.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-ui/issues"
  },
  "scripts": {
    "checkDeps": "npm-check -s; exit 0",
    "lint:styles": "stylelint 'src/**/*.css'",
    "lint:scripts": "tslint -p . -t stylish 'src/**/*.{ts,tsx}'",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "test": "npm run checkDeps && npm run lint",
    "storybook": "start-storybook -p 3001 -c .storybook",
    "build:storybook": "NODE_ENV=production build-storybook -c .storybook -o storybook-build",
    "build": "npm run test && ./scripts/buildPackage.sh",
    "prepublishOnly": "if [ -f README.md ]; then exit 1; fi",
    "createComponent": "./scripts/createComponent.sh"
  },
  "keywords": [
    "react-components",
    "ui",
    "styleless"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.51",
    "@types/prop-types": "^15.5.3",
    "@types/react": "16.4.2",
    "@types/react-transition-group": "^2.0.11",
    "react-input-mask": "^2.0.2",
    "react-transition-group": "^2.4.0",
    "stylable": "^5.4.10",
    "throttle-debounce": "^2.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-0": "^7.0.0-beta.51",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "@types/node": "^10.5.1",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__react": "^3.0.7",
    "autoprefixer": "^8.6.4",
    "awesome-typescript-loader": "^4.0.0",
    "babel-loader": "^8.0.0-beta",
    "browserslist-config-trigen": "^1.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.0.1",
    "eslint-config-trigen": "^3.2.0",
    "husky": "^1.0.0-rc.10",
    "npm-check": "^5.7.1",
    "postcss-loader": "^2.1.5",
    "postcss-reporter": "^5.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "stylable-integration": "^6.1.3",
    "stylelint": "^9.3.0",
    "stylelint-config-trigen": "^2.0.1",
    "svg-react-loader": "^0.4.5",
    "tslint": "^5.10.0",
    "tslint-config-trigen": "^3.0.5",
    "tslint-loader": "^3.6.0",
    "typescript": "2.9.1"
  }
}
