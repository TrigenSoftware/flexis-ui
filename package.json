{
  "name": "@flexis/ui",
  "version": "0.1.3",
  "description": "Styleless React Components",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/flexis-ui.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-ui/issues"
  },
  "browsers": "> 1%, last 2 versions, iOS > 7, Android > 4.4, not OperaMini all",
  "scripts": {
    "lint:styles": "stylelint 'src/**/*.css'",
    "lint:scripts": "tslint 'src/**/*.{ts,tsx}'",
    "test": "npm run lint:styles && npm run lint:scripts",
    "storybook": "start-storybook -p 3001 -c .storybook",
    "build:storybook": "NODE_ENV=production build-storybook -c .storybook -o storybook-build",
    "build": "./scripts/buildPackage.sh",
    "prepublishOnly": "if [ -f README.md ]; then exit 1; fi",
    "createComponent": "./scripts/createComponent.sh"
  },
  "keywords": [
    "react-components",
    "ui",
    "styleless"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.51",
    "react-input-mask": "^1.2.2",
    "react-transition-group": "^2.3.1",
    "stylable": "^5.3.5",
    "stylable-integration": "^6.1.2",
    "throttle-debounce": "^1.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
