  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@stylable/runtime": "^1.0.4",
    "@types/prop-types": "^15.5.5",
    "@types/react": "16.8.12",
    "@types/react-transition-group": "^2.0.13",
    "color": "^3.1.0",
    "react-input-mask": "^2.0.3",
    "react-transition-group": "^2.7.0",
    "resize-observer": "^1.0.0-alpha.1",
    "throttle-debounce": "^2.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@trigen/scripts": "^1.9.0",
