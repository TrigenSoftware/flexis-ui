{
	"exclude": "node_modules/**",
	"presets": [
		[
			"babel-preset-trigen",
			{
				"react": true,
				"reactRemovePropTypes": {
					"mode": "unsafe-wrap"
				}
			}
		]
	],
	"overrides": [
		{
			"test": "**/stylable/*.js",
			"presets": [
				[
					"babel-preset-trigen",
					{
						"commonjs": true
					}
				]
			]
		}
	],
	"env": {
		"test": {
			"presets": [
				[
					"babel-preset-trigen",
					{
						"targets": {
							"node": "current"
						},
						"commonjs": true,
						"react": true,
						"typescript": true
					}
				]
			]
		}
	}
}
